{
  "name": "react-playground",
  "version": "0.3.0",
  "description": "Simple web-site on React.js",
  "codename": "reactpg",
  "repository": "https://github.com/LestaD/react-webpack-playground.git",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build && mkdir ./build",
    "prestart": "npm install",
    "postinstall": "npm install -g roco",
    "start": "npm run dev",
    "dev": "webpack-dev-server --config webpack.config.js --port=7770",
    "build": "NODE_ENV=production webpack --colors --progress --config ./webpack.config.js"
  },
  "keywords": [
    "react",
    "web",
    "site"
  ],
  "author": "LestaD <i.am@lestad.net> (Sergey Sova)",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "6.0.3",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.29",
    "classnames": "^2.2.0",
    "css-loader": "^0.21.0",
    "csswring": "^4.0.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "history": "^1.12.6",
    "html-loader": "^0.3.0",
    "json-loader": "^0.5.3",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "postcss-loader": "^0.7.0",
    "poststylus": "^0.2.1",
    "react": "^0.14.0",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^2.0.0-alpha-4",
    "react-router": "^1.0.0-rc3",
    "rimraf": "^2.4.3",
    "source-map-support": "^0.3.3",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.0",
    "template-html-loader": "0.0.3"
  },
  "dependencies": {
    "webpack": "1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "babel": {
    "optional": [
      "es7.decorators",
      "es7.functionBind",
      "es7.classProperties",
      "es7.asyncFunctions"
    ]
  }
}
